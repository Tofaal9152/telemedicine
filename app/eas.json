{
  "cli": {
    "version": ">= 16.9.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_WEBSITE_URL": "https://telemedicine-olive.vercel.app"
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_WEBSITE_URL": "https://telemedicine-olive.vercel.app"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
